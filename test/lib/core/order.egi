(assert-equal "compare - case 1"
  (compare 10 10)
  <Equal>)

(assert-equal "compare - case 2"
  (compare 11 10)
  <Greater>)

(assert-equal "compare - case 3"
  (compare 10 11)
  <Less>)

(assert-equal "min"
  (min {10 20 5 20 30})
  5)

(assert-equal "min/fn"
  (min/fn compare {10 20 5 20 30})
  5)

(assert-equal "max"
  (max {10 20 5 20 30})
  30)

(assert-equal "max/fn"
  (max/fn compare {10 20 5 20 30})
  30)

(assert-equal "min-and-max"
  (min-and-max {10 20 5 20 30})
  [5 30])

(assert-equal "min-and-max/fn"
  (min-and-max/fn compare {10 20 5 20 30})
  [5 30])

(assert-equal "sort"
  (sort {10 20 5 20 30})
  {5 10 20 20 30})

(assert-equal "sort/fn"
  (sort/fn compare {10 20 5 20 30})
  {5 10 20 20 30})
